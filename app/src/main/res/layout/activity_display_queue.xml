<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/DarkLeaves"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.trider.smartbarui.DisplayQueue">

    <TextView
        android:text="@string/hello_world"
        android:id="@+id/queue_view"
        style="@style/CityTheme"
        android:layout_width="400dp"
        android:layout_height="400dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UpdateQueue"
        android:id="@+id/button20"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:onClick ="UpdateQueue"
        android:layout_marginEnd="77dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delete From Queue"
        android:onClick="DeleteQ"
        android:id="@+id/deleteQ"
        android:layout_below="@+id/button20"
        android:layout_alignStart="@+id/button20" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Number to Del from Q"
        android:textColor="@color/white"
        android:id="@+id/qText"
        android:layout_below="@+id/deleteQ"
        android:layout_alignStart="@+id/deleteQ" />


</RelativeLayout>
